--
-- PostgreSQL database cluster dump
--

SET default_transaction_read_only = off;

SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE azure_pg_admin;
ALTER ROLE azure_pg_admin WITH NOSUPERUSER INHERIT NOCREATEROLE NOCREATEDB NOLOGIN REPLICATION NOBYPASSRLS;
CREATE ROLE azure_superuser;
ALTER ROLE azure_superuser WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION BYPASSRLS;
CREATE ROLE councilbox;
ALTER ROLE councilbox WITH NOSUPERUSER INHERIT NOCREATEROLE CREATEDB LOGIN NOREPLICATION NOBYPASSRLS;
CREATE ROLE "councilbox-horus";
ALTER ROLE "councilbox-horus" WITH NOSUPERUSER INHERIT NOCREATEROLE CREATEDB LOGIN NOREPLICATION NOBYPASSRLS;
CREATE ROLE councilbox_readonly;
ALTER ROLE councilbox_readonly WITH NOSUPERUSER NOINHERIT NOCREATEROLE NOCREATEDB LOGIN NOREPLICATION NOBYPASSRLS;
CREATE ROLE psqladmin;
ALTER ROLE psqladmin WITH NOSUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION NOBYPASSRLS;
--
-- User Configurations
--

--
-- User Config "azure_superuser"
--

ALTER ROLE azure_superuser SET search_path TO 'pg_catalog';


--
-- Role memberships
--

GRANT azure_pg_admin TO councilbox GRANTED BY psqladmin;
GRANT azure_pg_admin TO "councilbox-horus" GRANTED BY psqladmin;
GRANT azure_pg_admin TO psqladmin GRANTED BY azure_superuser;




--
-- PostgreSQL database cluster dump complete
--

